class Roommate:
    def __init__(self, name, age, school, course):
        self.name = name
        self.age = age
        self.school = school
        self.course = course
    def introduce(self):
        return f"My roommate's name is {self.name}, {self.age} years old, studying {self.course} at {self.school}."
        abby = Roommate("Abby Kiumbe ", 20, "JKUAT", "GIS")
        print(abby.introduce())
    